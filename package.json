{
  "name": "pp-trees",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "download": "mkdir -p ./public/output && gsutil -m rsync -r gs://pp-trees-images ./public/output",
    "predev": "bun run download",
    "prebuild": "bun run download",
    "dev": "vite",
